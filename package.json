      "rollup": {
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
